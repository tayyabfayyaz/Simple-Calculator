@ECHO off
GOTO start
:find_dp0
SET dp0=%~dp0
EXIT /b
:start
SETLOCAL
CALL :find_dp0

IF EXIST "%dp0%\/usr/bin/evn.exe" (
  SET "_prog=%dp0%\/usr/bin/evn.exe"
) ELSE (
  SET "_prog=/usr/bin/evn"
  SET PATHEXT=%PATHEXT:;.JS;=;%
)

endLocal & goto #_undefined_# 2>NUL || title %COMSPEC% & "%_prog%" node "%dp0%\..\simple-calculator-jhm\index.js" %*
