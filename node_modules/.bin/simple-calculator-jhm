#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//usr/bin/evn" ]; then
  exec "$basedir//usr/bin/evn" node "$basedir/../simple-calculator-jhm/index.js" "$@"
else 
  exec /usr/bin/evn node "$basedir/../simple-calculator-jhm/index.js" "$@"
fi
